<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_employee_form_inherit_pph21" model="ir.ui.view">
        <field name="name">hr.employee.form.inherit.pph21</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr.view_employee_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='personal_information']" position="inside">
                <field name="dependents" position="after">
                    <field name="dependents"/>
                </field>
            </xpath>
        </field>
    </record>
</odoo>