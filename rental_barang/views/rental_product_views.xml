<odoo>

  <record id="view_product_product_rental_tree" model="ir.ui.view">
    <field name="name">product.product.tree.rental</field>
    <field name="model">product.product</field>
    <field name="inherit_id" ref="product.product_normal_form_view"/>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='name']" position="after">
        <field name="list_price"/>
      </xpath>
    </field>
  </record>

  <record id="action_product_product_rental" model="ir.actions.act_window">
    <field name="name">Produk Rental</field>
    <field name="res_model">product.product</field>
    <field name="view_mode">list,form</field>
  </record>

</odoo>
