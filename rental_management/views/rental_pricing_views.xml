<odoo>
    <record id="view_rental_pricing_tree" model="ir.ui.view">
        <field name="name">rental.pricing.tree</field>
        <field name="model">rental.pricing</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="duration"/>
                <field name="unit"/>
                <field name="price"/>
            </list>
        </field>
    </record>

    <record id="view_rental_pricing_form" model="ir.ui.view">
        <field name="name">rental.pricing.form</field>
        <field name="model">rental.pricing</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="rental_product_id" invisible="1"/>
                        <field name="duration"/>
                        <field name="unit"/>
                        <field name="price"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>